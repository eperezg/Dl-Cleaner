<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>dl-cleaner</portlet-name>
		<display-name>Document And Library Cleaner</display-name>
		<portlet-class>
			com.liferay.dl.cleaner.portlet.DlCleanerPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
        <supported-locale>es_ES</supported-locale>
        <resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Document And Library Cleaner</title>
			<short-title>Document And Library Cleaner</short-title>
			<keywords>Document And Library Cleaner</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	
</portlet-app>